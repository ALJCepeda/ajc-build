{
  "name": "ajc-build",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "docker-compose up -d",
    "stop": "docker-compose down",
    "deploy": "npm run build && npm run create:logs && npm run push",
    "push": "cd app && gcloud -q app deploy configs/app.yaml",
    "build": "ts-node src/index.ts",
    "create:logs": "mkdir app/logs && touch app/logs/access.log && touch app/logs/console.log && touch app/logs/error.log",
    "local": "cd app && node dist/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^8.1.0",
    "ncp": "^2.0.0",
    "nodemon": "^1.19.3",
    "rimraf": "^3.0.0",
    "simple-git": "^1.126.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/node": "^12.7.12",
    "@types/rimraf": "^2.0.2"
  }
}
